<%- include('../partials/header') %>


<!-- <h1><%= title %></h1> -->

<table id="list">
    <thead>
      <tr>
        <th>Brand</th>
        <th>Image</th>
      </tr>
    </thead>
    <tbody>
      <% item.forEach(function(i) { %>
        <tr>
          <td><%= i.brand %></td>
          <td><a href="/closet/<%=i._id%>"><img src="<%= i.image %>"></a></td>
        </tr>
        <% }); %>
      </tbody>
    </table>

<%- include('../partials/footer') %>
